pipeline {
   agent any
   environment {
      ENV = credentials('envtest')
   }
   stages {
       stage('Build Code') {
           steps {
               sh """
               echo "Building Artifact from Develop Branch"
               """
           }
       }
      stage('Deploy Code') {
          steps {
               sh """
               echo "Deploying Code from Develop Branch"
               """
               sh 'echo "passwordnya apa ya : $ENV" > test.txt'
               sh 'env | sort'
               sh 'sudo cp -r $WORKSPACE/ /home/gapta/jenkins-test/'
          }
      }
   }
}
